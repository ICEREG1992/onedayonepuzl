{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/puzzle",
      "destination": "/puzzle.html"
    }, {
      "source": "/list",
      "destination": "/list.html"
    }]
  }
}
